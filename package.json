{
  "name": "propel-sample-app-nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "format": "prettier --ignore-path .gitignore --write .",
    "lint": "eslint --fix .",
    "precommit": "yarn run lint && yarn run format",
    "prepare": "husky install",
    "gitlog": "git log origin..HEAD --reverse --format=\"âœ… %s%n%b %N%n\" > git-log.txt && code git-log.txt"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.css": [
      "prettier --write"
    ],
    "**/*.md": [
      "prettier --write"
    ],
    "**/*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "^2.16.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.13.7",
    "@mui/material": "^5.13.6",
    "@mui/x-date-pickers": "^6.9.1",
    "@propeldata/react-counter": "^0.31.0-rc.0",
    "@propeldata/react-leaderboard": "^0.5.4",
    "@propeldata/react-time-series": "^0.31.0-rc.0",
    "date-fns": "^2.30.0",
    "graphql": "^16.7.1",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "next": "^13.5.6",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "simple-oauth2": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^8.44.0",
    "eslint-config-next": "^13.4.7",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8"
  }
}
