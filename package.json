{
  "name": "propel-sample-app-nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "format": "prettier --ignore-path .gitignore --write .",
    "lint": "eslint --fix .",
    "precommit": "yarn run lint && yarn run format",
    "prepare": "husky install",
    "gitlog": "git log origin..HEAD --reverse --format=\"âœ… %s%n%b %N%n\" > git-log.txt && code git-log.txt"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.css": [
      "prettier --write"
    ],
    "**/*.md": [
      "prettier --write"
    ],
    "**/*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/lab": "^5.0.0-alpha.89",
    "@mui/material": "^5.13.6",
    "@mui/x-date-pickers": "^6.9.0",
    "@propeldata/react-counter": "^0.26.6",
    "@propeldata/react-time-series": "^0.29.9",
    "date-fns": "^2.28.0",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "graphql-tag": "^2.12.6",
    "next": "^12.2.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^18.2.0",
    "simple-oauth2": "^4.3.0"
  },
  "devDependencies": {
    "eslint": "^8.19.0",
    "eslint-config-next": "^12.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  }
}
